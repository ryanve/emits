{
  "name": "energy",
  "description": "Simple cross-platform event emitter",
  "version": "0.7.0",
  "homepage": "https://github.com/ryanve/energy",
  "license": "MIT",
  "author": "Ryan Van Etten",
  "keywords": [
    "eventemitter",
    "events",
    "event",
    "listener",
    "emitter",
    "emit",
    "ender",
    "server",
    "browser",
    "simple"
  ],
  "main": "./energy.js",
  "ender": "noop",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanve/energy.git"
  },
  "devDependencies": {
    "aok": "^1.9.0",
    "universal": "^0.1.1",
    "grunt-contrib-uglify": "^0.9.2",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-jshint": "^0.11.0",
    "deep-equal": "^1.0.1"
  },
  "jshintConfig": {
    "asi": true,
    "boss": true,
    "browser": true,
    "debug": true,
    "devel": true,
    "eqnull": true,
    "evil": true,
    "expr": true,
    "globals": {
      "define": true,
      "ender": true
    },
    "jquery": true,
    "lastsemic": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxcomplexity": 15,
    "maxerr": 10,
    "maxlen": 115,
    "maxparams": 4,
    "node": true,
    "sub": true,
    "supernew": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "worker": true
  }
}
